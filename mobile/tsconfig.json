{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "downlevelIteration": true
  },
  "extends": "expo/tsconfig.base"
}
